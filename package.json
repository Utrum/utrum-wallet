{
  "name": "utrum-wallet",
  "version": "2.0.0",
  "author": "./LEGAL/AUTHORS",
  "description": "Utrum Wallet",
  "license": "GPL-2.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "start-dev": "WALLET_ENV=dev yarn dev",
    "start-staging": "WALLET_ENV=staging yarn dev",
    "start-preprod": "WALLET_ENV=preprod yarn dev",
    "start-prod-internal": "WALLET_ENV=prod-internal yarn dev",
    "start-prod-preico": "WALLET_ENV=prod-preico yarn dev",
    "start-prod-ico": "WALLET_ENV=prod-ico yarn dev",
    "build-dev": "WALLET_ENV=dev yarn build",
    "build-staging": "WALLET_ENV=staging yarn build",
    "build-preprod": "WALLET_ENV=preprod yarn build",
    "build-prod-internal": "WALLET_ENV=prod-internal yarn build",
    "build-prod-preico": "WALLET_ENV=prod-preico yarn build",
    "build-prod-ico": "WALLET_ENV=prod-ico yarn build",
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "eslint": "./node_modules/.bin/eslint src/"
  },
  "build": {
    "productName": "Utrum Wallet",
    "appId": "com.utrum.wallet",
    "directories": {
      "output": "build"
    },
    "publish": null,
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      {
        "from": "resources/licenses.json",
        "to": "Resources/licenses.json"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/512x512.png",
      "artifactName": "utrum-wallet-${version}-${env.WALLET_ENV}-mac.${ext}"
    },
    "win": {
      "icon": "build/icons/512x512.png",
      "artifactName": "utrum-wallet-${version}-${env.WALLET_ENV}-setup.${ext}"
    },
    "linux": {
      "icon": "build/icons",
      "artifactName": "utrum-wallet-${version}-${env.WALLET_ENV}-x86_64.${ext}"
    }
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "axios": "^0.16.1",
    "bignumber.js": "^6.0.0",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^3.3.2",
    "bitcoinjs-lib-zcash": "^3.3.2",
    "bluebird": "^3.5.1",
    "bootstrap": "^4.0.0",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-vue": "^2.0.0-rc.6",
    "chart.js": "^2.7.2",
    "coinselect": "^3.1.11",
    "crypto-js": "^3.1.9-1",
    "electron-debug": "^1.4.0",
    "electron-license": "^1.0.0",
    "electron-publisher-s3": "^20.8.1",
    "electron-spawn": "^5.0.0",
    "electrum-client": "https://github.com/Monaize/node-electrum-client.git#electrumx-persistence",
    "elliptic": "^6.4.0",
    "express": "^4.16.3",
    "fs": "^0.0.1-security",
    "glyphicons": "^0.2.0",
    "jquery": "^3.3.1",
    "js-sha256": "^0.9.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "libwallet-mnz": "https://github.com/Utrum/libwallet-oot",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "moment-timezone": "^0.5.14",
    "npm-license-crawler": "^0.1.8",
    "toml": "^2.3.3",
    "typeface-montserrat": "0.0.54",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vue": "^2.3.3",
    "vue-chartjs": "^3.4.0",
    "vue-clipboard2": "0.0.8",
    "vue-electron": "^1.0.6",
    "vue-loaders": "^2.0.0",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-qrcode-reader": "^0.7.2",
    "vue-qriously": "^1.1.1",
    "vue-router": "^2.5.3",
    "vue-toasted": "^1.1.24",
    "vuex": "^2.3.1",
    "wif": "^2.0.6",
    "yarn": "^1.9.4",
    "yui-lint": "^0.2.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^19.19.1",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-header": "^1.2.0",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jquery": "^3.3.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "material-design-icons": "^3.0.1",
    "mocha": "^5.0.5",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "popper.js": "^1.12.9",
    "sass-loader": "^6.0.6",
    "select2": "^4.0.6-rc.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-select": "^2.4.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vue2-filters": "^0.3.0",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
